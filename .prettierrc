{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"printWidth": 280,
	"proseWrap": "never",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"useTabs": true,
	"vueIndentScriptAndStyle": false,
	"trailingComma": "none",
	"endOfLine": "lf"
}
